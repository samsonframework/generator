{
    "name": "samsonframework/generator",
    "description": "SamsonFramework code generator",
    "keywords": [
        "code",
        "samsonframework",
        "generator"
    ],
    "authors": [
        {
            "name": "Vitaly Iegorov",
            "email": "egorov@samsonos.com"
        }
    ],
    "require-dev": {
      "phpunit/phpunit": "5.*",
      "satooshi/php-coveralls": "*"
    },
    "license": "Open Software License (OSL) v 3.0",
    "homepage": "http://samsonos.com/",
    "autoload": {
        "psr-4": {
            "samsonframework\\generator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "samsonframework\\generator\\tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
